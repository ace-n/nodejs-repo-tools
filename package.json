{
  "name": "nodejs-repo-tools",
  "version": "0.1.0",
  "description": "Tools used to maintain and test Node.js repositories in the GoogleCloudPlaftorm organization.",
  "repository": "https://github.com/GoogleCloudPlatform/nodejs-repo-tools",
  "private": true,
  "scripts": {
    "lint": "semistandard \"**/*.js\"",
    "test": "npm run lint"
  },
  "bin": {
    "samples": "./bin/samples"
  },
  "main": "src/index.js",
  "author": "Google Inc.",
  "contributors": [
    {
      "name": "Jason Dobry",
      "email": "jdobry@google.com"
    }
  ],
  "license": "Apache Version 2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "engines": {
    "node": ">=0.12.7"
  },
  "dependencies": {
    "async": "^2.0.1",
    "chalk": "^1.1.3",
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "proxyquire": "^1.7.10",
    "request": "^2.74.0",
    "string": "^3.3.1",
    "supertest": "^2.0.0",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "semistandard": "^8.0.0"
  }
}
